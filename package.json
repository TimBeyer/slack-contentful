{
  "name": "slack-contentful",
  "version": "0.2.4",
  "description": "Slack notifications for entries and assets",
  "main": "server.js",
  "scripts": {
    "test": "xo && nodeunit tests",
    "start": "node server.js",
    "coveralls": "jscoverage test && SLACK_CONTENTFUL_COVERAGE=1 nodeunit --reporter=lcov tests | coveralls"
  },
  "keywords": [
    "slack",
    "contentful",
    "node"
  ],
  "author": "Brandon Him",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.13.3",
    "dotenv": "^1.2.0",
    "express": "^4.13.3",
    "node-cache": "^3.0.0",
    "node-slack": "0.0.7",
    "nodeunit": "^0.9.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.6",
    "jscoverage": "^0.6.0",
    "nodeunit": "^0.9.1",
    "xo": "^0.12.1"
  },
  "xo": {
    "envs": [
      "node"
    ],
    "space": 4,
    "rules": {
      "new-cap": 0
    }
  }
}
